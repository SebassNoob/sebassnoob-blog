{
  "name": "sebassnoob-blog",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "bun run --env-file=.env ./scripts/generate-sitemap.ts && tsc && vite build ",
    "lint": "bunx eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "prettier": "bunx prettier --plugin=@prettier/plugin-xml --write .",
    "test": "cd cypress && bash -c './test.sh --no-cache'",
    "test:dev": "cd cypress && sh -c './test.sh --dev'"
  },
  "dependencies": {
    "fast-xml-parser": "^4.5.3",
    "localforage": "^1.10.0",
    "match-sorter": "^6.4.0",
    "react": "19",
    "react-dom": "19",
    "react-markdown": "10",
    "react-router-dom": "^6.29.0",
    "rehype-highlight": "^7.0.2",
    "remark-gfm": "^4.0.1",
    "sort-by": "^1.2.0",
    "tailwind-merge": "^3.0.2"
  },
  "devDependencies": {
    "@prettier/plugin-xml": "^3.4.1",
    "@tailwindcss/postcss": "^4.0.9",
    "@types/node": "^22.13.5",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "eslint": "^8.57.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.19",
    "postcss": "^8.5.3",
    "prettier": "^3.5.2",
    "sharp": "^0.33.5",
    "svgo": "^3.3.2",
    "tailwindcss": "^4.0.9",
    "typescript": "^5.7.3",
    "vite": "6",
    "vite-plugin-image-optimizer": "^1.1.8",
    "vite-tsconfig-paths": "^4.3.2"
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "endOfLine": "lf",
    "xmlWhitespaceSensitivity": "ignore"
  }
}
